{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "data": {
        "url": "enhanced_renewable_energy_projects.csv",
        "format": {
            "type": "csv"
        }
    },
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "State",
            "type": "nominal",
            "title": "State/Region"
        },
        "y": {
            "aggregate": "count",
            "field": "Project_Name",
            "type": "quantitative",
            "title": "Number of Projects"
        },
        "color": {
            "field": "State",
            "type": "nominal"
        }
    }
}